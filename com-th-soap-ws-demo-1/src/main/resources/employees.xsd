<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="https://www.torryharris.com/soap-ws-demo"
	targetNamespace="https://www.torryharris.com/soap-ws-demo"
	elementFormDefault="qualified">

	<xs:element name="getAllEmployeesRequest">
	</xs:element>

	<xs:element name="getAllEmployeesResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeesType" type="tns:employeeType"
					maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="getEmployeeRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeeId" type="xs:int" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="getEmployeeResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeeType" type="tns:employeeType" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="addEmployeeRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeeId" type="xs:int" />
				<xs:element name="firstName" type="xs:string" />
				<xs:element name="salary" type="xs:double" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="updateEmployeeRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeeId" type="xs:int" />
				<xs:element name="firstName" type="xs:string" />
				<xs:element name="salary" type="xs:double" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="deleteEmployeeRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeeId" type="xs:int" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="employeeType">
		<xs:sequence>
			<xs:element name="employeeId" type="xs:int" />
			<xs:element name="firstName" type="xs:string" />
			<xs:element name="salary" type="xs:double" />
		</xs:sequence>
	</xs:complexType>

</xs:schema>